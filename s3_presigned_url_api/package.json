{
  "name": "s3-presigned-url-api",
  "version": "1.0.0",
  "description": "AWS Lambda function for generating presigned S3 URLs for image uploads and downloads",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "package": "npm run build && cd dist && zip -r ../lambda.zip ."
  },
  "keywords": [
    "aws",
    "lambda",
    "s3",
    "presigned-url",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "3.614.0",
    "@aws-sdk/s3-request-presigner": "3.614.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.140",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-security": "3.0.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  }
}
